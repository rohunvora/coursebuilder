{
  "name": "coursebuilder",
  "version": "1.2.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "test": "jest --config config/jest.config.js",
    "postinstall": "prisma generate",
    "vercel-build": "./scripts/vercel-build.sh",
    "seed-achievements": "node scripts/seed-achievements.js",
    "seed-achievements:db": "node scripts/seed-achievements-db.js",
    "setup-db": "node scripts/setup-database.js",
    "setup-db:seed": "node scripts/setup-database.js --seed",
    "test:unit": "jest --config config/jest.config.js tests/unit",
    "test:integration": "jest --config config/jest.config.js tests/integration",
    "clean": "rm -rf .next .cache .tmp dist/*.zip",
    "package": "./scripts/package.sh"
  },
  "dependencies": {
    "@prisma/client": "^5.7.1",
    "@supabase/supabase-js": "^2.52.0",
    "canvas-confetti": "^1.9.2",
